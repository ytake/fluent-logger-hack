{
  "name": "ytake/fluent-logger-hack",
  "type": "library",
  "description": "A structured logger for Fluentd (Hack/HHVM)",
  "license": "MIT",
  "keywords": [
    "hack",
    "hhvm",
    "fluentd",
    "logger"
  ],
  "minimum-stability": "stable",
  "require": {
    "hhvm": "^4.8",
    "hhvm/hsl": "^4.0",
    "hhvm/hsl-experimental": "^4.0",
    "hhvm/hhvm-autoload": "^2.0.0"
  },
  "require-dev": {
    "hhvm/hacktest": "^1.5",
    "facebook/fbexpect": "^2.5.7",
    "hhvm/hhast": "^4.8.0"
  },
  "autoload": {
    "psr-4": {
      "Ytake\\Fluent\\Logger\\": "src/"
    }
  },
  "scripts": {
    "tests": [
      "hhvm ./vendor/bin/hacktest.hack tests/"
    ],
    "register": [
      "hhvm ./vendor/bin/hh-autoload.hack"
    ]
  }
}
